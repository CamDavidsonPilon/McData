<script type="text/javascript">

    //protected bug on IE
    $("#niLocations div.navpane, #niCompanyInfo div.navpane, #niFreshOffers div.navpane, #niMenu div.navpane").css({
        'left': '-5000px',
        'visibility': 'visible',
        'display': ''
    });

    function initAreaBoxes(parent) {
        if (typeof (parent) == 'undefined')
            parent = $('ul#MainNavigation');

        $('.content', parent).each(function () {
            var areaBoxes = $('> .areaBox', this);
            areaBoxes.removeClass('first').removeClass('last');
            areaBoxes.eq(0).addClass('first');
            areaBoxes.eq(-1).addClass('last');

            $('> .areaBox > .areaBox', this).removeClass('first').removeClass('last');
            $('> .areaBox > .areaBox:first-child', this).addClass('first');
            $('> .areaBox > .areaBox:last-child', this).addClass('last');

            makeSameHeight(areaBoxes);
        });
    }

    initializeMenuNavigator();

    function initializeMenuNavigatorNoAjax() {

        var menuElements = "";
        var cat = $.parseJSON(menu_cat);
        for (i = 0; i < cat.length; i++) {
            var categoryId = cat[i].ID;
            var displayTitle = cat[i].DisplayTitle;

            var strCategory = toSeoFriendly(stripHtml(displayTitle) + " " + categoryId);

            menuElements += '<ul class="category areaBox">';
            menuElements += '<li class="root"><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/index.html" class="titleLink lnkNutrition category' + categoryId + ' first-list">' + displayTitle + '</a></li>';

            //menuElements += retrieveSubCategories(strCategory, categoryId, categoryDetailUrl);
            for (j = 0; j < cat[i].subcats.length; j++) {
                var subCategoryId = cat[i].subcats[j].ID;
                var title = cat[i].subcats[j].Title;
                var strSubCategory = toSeoFriendly(stripHtml(title) + " " + subCategoryId);
                menuElements += '<li><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/' + strSubCategory + '/index.html" class="lnkNutrition category' + subCategoryId + ' first-list">' + title + '</a></li>';
            }

            menuElements += '</ul>';
        }

        $(menuElements).insertBefore($("#pdfdownload"));

        $('ul#MainNavigation > li.navitem#niMenu .content a.titleLink').each(function () {
            if ($('img.arrow', this).length == 0) {
                $(this).append('<img class="arrow" src="/images/nav/link_arrow.png" border=0  width=5 height=9 />');
            }
        });

        initAreaBoxes($('ul#MainNavigation > li.navitem#niMenu'));
    }


    function initializeMenuNavigator() {

        if (typeof menu_cat != 'undefined') {
            initializeMenuNavigatorNoAjax();
            return;
        }


        $.ajax({
            url: '/cms' + CulturePrefix + 'cms_out/menu_nutrition/categories/all.xml',
            datatype: 'xml',
            type: 'get',
            success: function (data) {
                var menuElements = "";
                $(data).find('MenuCategory').each(function () {
                    var categoryId = $(this).attr('ID');
                    var displayTitle = $(this).children('DisplayTitle').text();
                    var categoryDetailUrl = $(this).children('DetailsUrl').text();

                    var strCategory = toSeoFriendly(stripHtml(displayTitle) + " " + categoryId);
                    menuElements += '<ul class="category areaBox">';
                    menuElements += '<li class="root"><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/index.html" class="titleLink lnkNutrition category' + categoryId + ' first-list">' + displayTitle + '</a></li>';
                    menuElements += retrieveSubCategories(strCategory, categoryId, categoryDetailUrl);
                    menuElements += '</ul>';
                });

                $(menuElements).insertBefore($("#pdfdownload"));

                $('ul#MainNavigation > li.navitem#niMenu .content a.titleLink').each(function () {
                    if ($('img.arrow', this).length == 0) {
                        $(this).append('<img class="arrow" src="/images/nav/link_arrow.png" border=0  width=5 height=9 />');
                    }
                });

                initAreaBoxes($('ul#MainNavigation > li.navitem#niMenu'));
            }
        });
    }

    function retrieveSubCategories(strCategory,  rootCategoryId, categoryDetailUrl) {
        var menuElements = "";
        $.ajax({
            url: categoryDetailUrl,
            datatype: 'xml',
            type: 'get',
            async: false,
            success: function (data) {
                $(data).find('SubCategory').each(function () {
                    var subCategoryId = $(this).attr('ID');
                    var title = $(this).attr('Title');
                    var strSubCategory = toSeoFriendly(stripHtml(title) + " " + subCategoryId);

                    menuElements += '<li><a href="' + CulturePrefix + 'menu-nutrition/' + strCategory + '/' + strSubCategory + '/index.html" class="lnkNutrition category' + subCategoryId + ' first-list">' + title + '</a></li>';
                });
            }
        });
        return menuElements;
    }
</script>

