<script type="text/javascript">

    function OnSearchSubmit() {
        return ($("#main_search").val().trim().toLowerCase() != "search")
    }

    $(document).ready(function () {
        restaurantController.registerLocationResolutionCallback(function initMyMealYourBKInfo(objRestaurant, objIPGeolocatedLatLng) {
            freshOffersController.registerLoadCallback(function (promotionItems) {
                $('#FooterOffers > ul').empty();

                var featuredItems = promotionItems.filter(function () { return this.IsFooterFeatured && this.IsAvailableAtThisTime() && this.IsAvailableAtRestaurant(objRestaurant); })
                                                  .slice(0, 2);
                featuredItems.each(function () {
                    var li = $('<li class="promoitem"></li>');

                    if (this.FooterFeaturedHtmlOverride != '') {
                        li.append(this.FooterFeaturedHtmlOverride);
                    } else {
                        var aTagAttrs = 'href="' + this.GetRefItemUrl() + '" target="' + this.GetRefItemTarget() + '"';

                        li.append('<div class="text">' +
                                    (this.FooterFeaturedTitleText == '' ? '' : ('<div class="title"><a ' + aTagAttrs + '>' + this.FooterFeaturedTitleText + '</a></div>')) +
                                    (this.FooterFeaturedLinkText == '' ? '' : ('<div class="link"><a ' + aTagAttrs + '>' + this.FooterFeaturedLinkText + '</a></div>')) +
                                  '</div>' +
                                  (this.FooterFeaturedImagePath == '' ? '' : ('<div class="image"><a ' + aTagAttrs + '><img src="' + this.FooterFeaturedImagePath + '" border="0" /></a></div>')) +
                                  (this.FooterFeaturedDisclaimerText == '' ? '' : ('<div class="disclaimer">' + this.FooterFeaturedDisclaimerText + '</div>'))
                                 );
                    }

                    $('#FooterOffers > ul').append(li);
                });

                $("#FooterOffers .promoitem:first").addClass('first');
                $('#FooterOffers a.video').modalVideo();
            });
        });

        $("#FooterNavigation ul > li:first-child").addClass('first');
        $("#FooterNavigation ul > li:last-child").addClass('last');
        $("#FooterSubNavigation ul > li:first-child").addClass('first');
        $("#FooterSubNavigation ul > li:last-child").addClass('last');

        if ($.browser.msie && $.browser.version == "7.0") {
            // Fix Sign up for BK in ES, IE7 ... widthFix only available in es
            $("#FooterBottomRight #SignUpBox a").addClass("widthFix");
            $("#FooterSubNavigation ul").css("white-space", "nowrap");
        }
    });
</script>

