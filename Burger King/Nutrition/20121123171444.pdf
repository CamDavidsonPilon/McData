<script type="text/javascript">

    $(document).ready(function () {

        //protected bug in IE9
        $("#niLocations div.navpane, #niCompanyInfo div.navpane, #niFreshOffers div.navpane, #niMenu div.navpane").css({
            'left': '-5000px',
            'visibility': 'visible',
            'display': ''
        });

        $('a.nolink').attr('href', '#');
        $('a.nolink').click(function (e) {
            e.preventDefault();
        });

        $(document).click(function (e) {
            var target = $(e.target);
            var parents = target.parents('li.navitem');

            if (parents.length <= 0) {
                $("ul#MainNavigation > li").each(function (k, v) {
                    if ($(v).hasClass("hover")) {
                        OnNavMenuUnClick($(v));
                    }
                });
            }
        });

        $("ul#MainNavigation > li.navitem").click(function (e) { //onmousein

            var target = $(e.target);

            if ($(this).hasClass("hover")) {
                if ((target.is('li.navitem > a.nolink') && target.parents('li.navitem').length > 0) || target.is('li.navitem')) {
                    OnNavMenuUnClick($(this));
                }
            } else {
                OnNavMenuClick($(this));
            }

        });

        function OnNavMenuClick(nav) {

            $("ul#MainNavigation > li.navitem").each(function (k, v) {
                if ($(v).hasClass("hover")) {
                    OnNavMenuUnClick($(v));
                }
            });

            nav.addClass("hover");
            nav.find("> a.nolink").addClass("nolink_actived");

            var navpane = $('div.navpane', nav);
            navpane.css('display', '');

            var cw = $('.content', navpane).outerWidth(false);
            var ch = $('.content', navpane).outerHeight(false);
            $('.border_topcenter', navpane).css('width', (cw - 10) + 'px');
            $('.border_bottomcenter', navpane).css('width', (cw - 10) + 'px');
            $('.border_contentleft', navpane).css('height', ch + 'px');
            $('.border_contentright', navpane).css('height', ch + 'px');

            var wMainNav = $('ul#MainNavigation').outerWidth(true);
            var wNavItem = nav.outerWidth(true);
            var wNavPane = navpane.outerWidth(true);
            var lNavItem = nav.position().left;
            var left;
            if (navpane.hasClass('full_width') || navpane.hasClass('align_left')) {
                left = 0;
            } else if (navpane.hasClass('align_right')) {
                left = wMainNav - wNavPane;
            } else {
                left = lNavItem + (wNavItem - wNavPane) / 2;
            }

            if (left < 0) {
                left = 0;
            } else if (left + wNavPane > wMainNav) {
                left = wMainNav - wNavPane;
            }

            navpane.css({
                'left': left + 'px',
                'visibility': 'visible'
            });

            var navpaneArrow = $('div.navpane > .arrow', nav);
            navpaneArrow.css({
                'left': (lNavItem + wNavItem / 2 - left - navpaneArrow.width() / 2) + 'px'
            });
        }

        function OnNavMenuUnClick(nav) {
            nav.removeClass("hover");
            $('div.navpane', nav).css("visibility", "hidden");
            $('div.navpane', nav).css("display", "none");
            nav.find("> a.nolink").removeClass("nolink_actived");
        }

        $("ul.dropdown li ul li:has(ul)").find("a:first").append(" &raquo; ");

        initAreaBoxes();

        window.setTimeout(function () {
            initAreaBoxes();
            $("ul#MainNavigation div.navpane").each(function () {
                // Border
                // - top left
                $(this).append("<div class='border_topleft'></div>");
                // - top center
                $(this).append("<div class='border_topcenter'></div>");
                // - top right
                $(this).append("<div class='border_topright'></div>");
                // - content left
                $(this).append("<div class='border_contentleft'></div>");
                // - content right
                $(this).append("<div class='border_contentright'></div>");
                // - bottom left
                $(this).append("<div class='border_bottomleft'></div>");
                // - bottom center
                $(this).append("<div class='border_bottomcenter'></div>");
                // - bottom right
                $(this).append("<div class='border_bottomright'></div>");
                // Arrow
                $(this).append("<div class='arrow'></div>");

                var cw = $('.content', this).outerWidth(false);
                var ch = $('.content', this).outerHeight(false);
                $('.border_topcenter', this).css('width', (cw - 10) + 'px');
                $('.border_bottomcenter', this).css('width', (cw - 10) + 'px');
                $('.border_contentleft', this).css('height', ch + 'px');
                $('.border_contentright', this).css('height', ch + 'px');
            });
        }, 500);

        if (location.href.contains('/menu-nutrition/')) {
            $('#niMenu').addClass('active');
        }
        if (location.href.contains('/fresh-offers/')) {
            $('#niFreshOffers').addClass('active');
        }
        if (location.href.contains('/company-info/')) {
            $('#niCompanyInfo').addClass('active');
        }
        if (location.href.contains('/restaurant-locator/')) {
            $('#niLocations').addClass('active');
        }
		
		var userAgent = navigator.userAgent.toLowerCase();
        $.browser = {
			version: (userAgent.match(/.+(?:rv|it|ra|ie|me)[\/: ]([\d.]+)/) || [])[1],
			chrome: /chrome/.test(userAgent),
			safari: /webkit/.test(userAgent) && !/chrome/.test(userAgent),
			opera: /opera/.test(userAgent),
			msie: /msie/.test(userAgent) && !/opera/.test(userAgent),
			mozilla: /mozilla/.test(userAgent) && !/(compatible|webkit)/.test(userAgent)
		};
		
        // Implement location top menu bar fix, for non-chrome browser
        if (!$.browser.chrome) {
             $("ul#MainNavigation li#niShare").next().css("padding-right", "2px");
        }
    });
</script>

<script type="text/javascript">
    var globalHeaderLocation = BK.globalHeaderLocation;
    var headerLocationHasGeocodedResult = false;
    
    function headerRestaurantLocatorOnKeyPress(control, e) {
        if (e.keyCode == 13 && $(control).val() != '') {
            // Use timeout to wait for form auto complete
            setTimeout(headerGetFormattedAddressFromGeocoder, 250);
            return false;
        } else {
            return true;
        }
    }

    function headerGetFormattedAddressFromGeocoder() {
        geocoder = new google.maps.Geocoder();
        geocoder.geocode({ 'address': $("#nav_find_bk").val(), 'region': CultureCountry },
                            function (response, status) {
                                if (status == google.maps.GeocoderStatus.OK) {
                                    $("#nav_find_bk").val(response[0].formatted_address);
                                }
                                headerLocationHasGeocodedResult = true;
                                $("#form_find_bk").submit();
                            });
    }

    $(document).ready(function () {
        restaurantController.registerLocationResolutionCallback(function () {
            globalHeaderLocation.initialize();
        });
    });
</script>

