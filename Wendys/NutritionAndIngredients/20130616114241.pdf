	<script type="text/javascript">
	$(document).ready(function(){
		if(localStorage.getItem('LocationLat')!=null && localStorage.getItem('LocationLong')!=null){
			var userLat = localStorage.getItem('LocationLat');
			var userLong = localStorage.getItem('LocationLong');
				if($('#userLat')!=null && $('#userLong')!=null){
					$('#userLat').val(userLat);
					$('#userLong').val(userLong);
				}
		} else {
				if($('#userLat')!=null && $('#userLong')!=null){
					$('#userLat').val(jsProperties.userLat);
					$('#userLong').val(jsProperties.userLong);
				}
				localStorage.setItem('LocationLat',jsProperties.userLat);
				localStorage.setItem('LocationLong',jsProperties.userLong);
		}

		//On change of location on Careers Search bar
		$("#city").change(function(){
			$("#city").val($("#city").val());
			localStorage.setItem('CareersCity', $("#city").val());
		});

		$('select#state').change(function(e) {
			localStorage.setItem('CareersState', $("#state").val());
			$("#state").val($("#state").val());
		    /* $('span.location_state').text(e.target.options[e.target.selectedIndex].value); */
		    $("select#state option",this).each(function(){
				if($(this).val() == e.target.options[e.target.selectedIndex].value){
					$(this).attr('selected', true);
				}
			});
		});
		$('select#categories').change(function(e) {
			localStorage.setItem("category", $("#categories").val());
			$("select#categories option",this).each(function(){
				$(this).attr('selected', true);
			});
		});
		$('select#radius').change(function(e) {
			localStorage.setItem("radius",$("#radius").val());
			$("select#radius option",this).each(function(){
				$(this).attr('selected', true);
			});
		});
	});
	if(localStorage.getItem('CareersState')!=null && localStorage.getItem('CareersState')!="0" && showLocation === true){
		$('span.location_city').text(localStorage.getItem('CareersCity'));
		$('span.location_state').text(localStorage.getItem('CareersState'));
		$('.location_based').show();
	}
	</script>
	
