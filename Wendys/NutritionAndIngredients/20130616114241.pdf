	<script type="text/javascript">
			var showLocation = false;
			$(document).ready(function(){
				if(localStorage.getItem('CareersCity')!=null && localStorage.getItem('CareersState')!=null){
					var city = localStorage.getItem('CareersCity');
					var state = localStorage.getItem('CareersState');
						if($('#city')!=null && $('#state')!=null){
							var stateText;
							$('#city').val(city);
							$('#state').val(state);
							$("select#state option",this).each(function(){
								if($(this).val() == state){
									$(this).attr('selected', true);
									stateText = $(this).text();
								}
							});
							$('#state').parent().find('.styled_select .text').text(stateText);
						}
				} else {
					jsProperties.isUserGeoLocated = false;
					if(localStorage.getItem('CareersCity')!=null && localStorage.getItem('CareersState')!=null){
						$('#city').val(localStorage.getItem('CareersCity'));
						$('#state').val(localStorage.getItem('CareersState'));
						$("select#state option",this).each(function(){
							if($(this).val() == localStorage.getItem('CareersState')){
								$(this).attr('selected', true);
							}
						});
					} else {
						$('#city').val(jsProperties.userDefaultCity);
						$('#state').val(jsProperties.userDefaultState);
						localStorage.setItem('CareersCity', jsProperties.userDefaultCity);
						localStorage.setItem('CareersState', jsProperties.userDefaultState);
						$("select#state option",this).each(function(){
							if($(this).val() == jsProperties.userDefaultState){
								$(this).attr('selected', true);
							}
						});
					}
				}
				if($('#radius')!=null){
					var radius = localStorage.getItem('radius');
					$("select#radius option",this).each(function(){
						if($(this).val() == radius){
							$(this).attr('selected', true);
						}
					});
				}
				if($('#category')!=null){
					var category = localStorage.getItem('category');
					$("select#categories option",this).each(function(){
						if($(this).val() == category){
							$(this).attr('selected', true);
						}
					});
				}

			});
	</script>
	
